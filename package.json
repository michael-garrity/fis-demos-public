{
  "name": "fis-demos",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "eslint --fix",
    "fix:sql": "./scripts/sqlfluff fix",
    "lint": "eslint src",
    "lint:db": "supabase db lint",
    "lint:sql": "./scripts/sqlfluff lint",
    "lint:yaml": "./scripts/yamllint .",
    "test": "vitest --reporter=html --coverage",
    "test:cli": "vitest --coverage",
    "test:integration": "vitest test",
    "test:serial": "vitest --coverage --no-file-parallelism",
    "test:unit": "vitest spec",
    "types:check": "tsc --noEmit --pretty",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report",
    "prepare": "husky",
    "db:dump": "./scripts/db-dump",
    "db:migrate": "supabase migration up && npm run db:sync",
    "db:reset": "supabase db reset && npm run db:sync",
    "db:reset:test": "node scripts/db-reset-test.js",
    "db:start": "supabase start",
    "db:status": "supabase status",
    "db:status:env": "./scripts/db-env",
    "db:sync": "npm run db:reset:test && npm run db:dump && npm run db:types",
    "db:types": "supabase gen types typescript --local > src/types/database.ts"
  },
  "dependencies": {
    "@heroui/react": "^2.8.5",
    "@sentry/nextjs": "^10.29.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0",
    "next": "16.0.10",
    "next-router-mock": "^1.0.4",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "husky": "^9.1.7",
    "inflection": "^3.0.2",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "pg-schema-dump": "^2.0.2",
    "supabase": "^2.72.8",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  }
}
